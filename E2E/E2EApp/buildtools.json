{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "artifactCopy": {
    "disable": true
  },
  "appConfig": {
    "strategy": "BundleAll"
  },
  "automaticVersioning": {
    "behavior": "PreferBuildNumber",
    "environment": "All",
    "versionOffset": 0,
    "disable": false
  },
  "css": {
    "minify": false,
    "bundleScss": false,
    "disable": false
  },
  "images": {
    "directories": [
      "Images\\Beta",
      "Images\\Shared"
    ],
    "conditionalDirectories": {
      "MonoAndroid": [ "Images\\Android" ],
      "Xamarin.iOS": [ "Images\\iOS" ]
    },
    "watermarkOpacity": null,
    "disable": false
  },
  "manifests": {
    "token": "$$",
    "variablePrefix": "Manifest_",
    "missingTokensAsErrors": false,
    "disable": false
  },
  "releaseNotes": {
    "maxDays": 7,
    "maxCommit": 10,
    "characterLimit": 250,
    "filename": "ReleaseNotes.txt",
    "createInRoot": true,
    "disable": false
  },
  "projectSecrets": {
    "E2EApp": {
      "disable": false,
      "delimiter": ";",
      "prefix": "BuildTools_",
      "className": "Secrets",
      "namespace": "Helpers",
      "properties": [
        {
          "name": "SampleString",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SampleBool",
          "type": "Bool",
          "isArray": false
        },
        {
          "name": "SampleInt",
          "type": "Int",
          "isArray": false
        },
        {
          "name": "SampleDouble",
          "type": "Double",
          "isArray": false
        },
        {
          "name": "SampleUri",
          "type": "Uri",
          "isArray": false
        }
      ]
    }
  },
  "debug": false
}